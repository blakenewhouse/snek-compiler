(fun (sum_up_to n)
(let ((i 0) (acc 0))
(loop (if (> i n)
(break acc)
(block (set! acc (+ acc i)) (set! i (+ i 1)))))))

(fun (factorial n)
(let ((i 1) (acc 1))
(loop (if (> i n)
(break acc)
(block (set! acc (* acc i)) (set! i (+ i 1)))))))

(fun (print_table n)
(let ((i 1))
(loop (if (> i n)
(break i)
(block (print (sum_up_to i)) (print (factorial i))
(set! i (+ i 1))
)
)
)
)
)

(print_table 7)